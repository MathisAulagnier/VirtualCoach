{
  "expo": {
    "name": "mycaoch",
    "slug": "mycaoch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2D96F2",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#2D96F2"
          },
          "imageWidth": 50
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.virtualcoach.mycoach"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png"
      },
      "package": "com.virtualcoach.mycoach"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "74ca4375-1ffd-47bb-96d9-91e7b5f1dcfa"
      }
    }
  }
}
